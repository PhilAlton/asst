    "php": ">=7.0.15",
    "ext-pdo": "*",
    "defuse/php-encryption": "^2.0",
    "phpdocumentor/phpdocumentor": "^2.9",
  },
  "license": "Copyright Helix Tech Ltd, 2016",
  "authors": [
    {
      "name": "Dr Philip Alton",
      "email": "phil.alton@helix-tech.co.uj"
    }
  ],
  "autoload": {
    "psr-4": { "HelixTech\\asstAPI\\": "HelixTech/asstAPI/" }
  },
  "archive": {
